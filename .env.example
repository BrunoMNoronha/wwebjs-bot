# Configuração de fluxos
MENU_FLOW=1

# Configuração de administração
OWNER_ID=
ALLOW_SELF_ADMIN=0

# Rate limiting
RATE_LIMIT_PER_CHAT_MS=1500
RATE_LIMIT_GLOBAL_MAX=10
RATE_LIMIT_GLOBAL_INTERVAL_MS=1000

# Configuração de timeout de fluxos
FLOW_PROMPT_WINDOW_MS=120000
FLOW_TTL_SECONDS=1800

# Redis (opcional, para persistência de estado)
REDIS_URL=
REDIS_HOST=
REDIS_PORT=6379
REDIS_PASSWORD=
FLOW_REDIS_PREFIX=wwebjs:flow:

# Ambiente
NODE_ENV=development